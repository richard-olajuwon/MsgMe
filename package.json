  "main": "index.js",
  "scripts": {
    "start": "concurrently \"npm run server-install\" \"npm run server\" \"npm run socket-install\" \"npm run socket\"",
    "server": "node backend/server.js",
    "server-install": "npm install --force --prefix backend",
    "client-install": "npm install --force --prefix frontend",
    "socket-install": "npm install --force --prefix socket",
    "socket": "npm start --prefix socket",
    "client": "npm start --prefix frontend",
    "client-fix": "npm audit fix --force --prefix frontend",
    "dev": "concurrently \"npm run server\" \"npm run socket\""
  },
